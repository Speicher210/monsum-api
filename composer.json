{
    "name": "speicher210/monsum-api",
    "type": "library",
    "description": "API for Monsum",
    "keywords": [
        "monsum",
        "fastbill"
    ],
    "homepage": "https://github.com/Speicher210/monsum-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Dragos Protung",
            "email": "dragos@protung.ro",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^5.6|^7.0",
        "guzzlehttp/guzzle": "^6.1",
        "jms/serializer": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Speicher210\\Monsum\\Api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Speicher210\\Monsum\\Test\\Api\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "cs-check": "phpcs --warning-severity=0",
        "cs-fix": "phpcbf -p"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
